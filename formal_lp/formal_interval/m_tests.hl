let rec n_mono res =
  match res with
    | Result_pass_mono _ -> 1
    | Result_glue (_, _, r1, r2) -> n_mono r1 + n_mono r2
    | _ -> 0;;

n_mono certificate;;
let rec print_pass res =
  match res with
    | Result_pass (_, x, z) -> report (string3 (x,z,""))
    | Result_glue (_, _, r1, r2) -> print_pass r1; print_pass r2
    | _ -> ();;
print_pass certificate;;

