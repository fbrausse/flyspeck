    |2 ->[`&2 * h0`;`&2`;`&2 * h0`;`&2`] 
    | _ ->  failwith "make_hexB" in
{
id = Printf.sprintf "GYQVFXJ hexB %d" i;
doc = "This inequality is the main hexagon inequality
