    (INTRO_TAC Nonlinear_lemma.vol3_vol_x [`x1`;`x2`;`x3`;`x4`;`x5`;`x6`]) in
    EVERY [
      TAC_ARC;
      TAC_ARC123;
      MUD_TAC;
      INTRO_0;
      INTRO_1;
      INTRO_VOL3;
      TAC126;
      TAC_VOL3;
      TAC135;
      TAC456;
      TAC123;
      ASM_SIMP_TAC[REAL_LT_IMP_LE];
