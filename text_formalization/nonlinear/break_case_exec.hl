  ]);;
  (* }}} *)

let frac_left_rules = prove_by_refinement(
  `!i h t r. frac_left i r [] = [] /\
      frac_left 0 r (h::t) = (FST h,FST h * (&1 - r) + SND h * r)::t /\
      frac_left (SUC i) r (h::t) = h :: frac_left i r t`,
  (* {{{ proof *)
  [
