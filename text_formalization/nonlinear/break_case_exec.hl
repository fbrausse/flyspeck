let get_gamma_tactic s =
  let t = get_ineq s in
    PREP_TAC THEN DTAC t THEN POST_DTAC THEN 
	    ETA126_TAC THEN ETA135_TAC THEN 
	    TRY FULL8_TAC THEN TRY KEEP135_TAC THEN 
	    REPLICATE_TAC 4 (POP_ASSUM kill) THEN POST_TAC THEN REPEAT DISCH_TAC;;

let needs_gamma s = 
  let t = get_ineq s in
    needs_delta_ineq2 t;;

let run_gamma s = 
