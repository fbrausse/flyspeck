let bisect_right_rules = prove_by_refinement(
  `!i h t. bisect_right i [] = [] /\
      bisect_right 0 (h::t) = (pathR h)::t /\
      bisect_right (SUC i) (h::t) = h :: bisect_right i t`,
  (* {{{ proof *)
  [
