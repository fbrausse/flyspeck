let bisect_thm = prove_by_refinement(
  `!d i x f. LENGTH x = LENGTH d ==>
    ineqm x (bisect_left i d) f ==> ineqm x (bisect_right i d) f ==> ineqm x d f`,
  (* {{{ proof *)
  [
