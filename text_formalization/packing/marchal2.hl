 (DEL_TAC THEN DEL_TAC);
 (UP_ASM_TAC THEN REWRITE_WITH `S INTER ball (v,r) = ball (v:real^3,r) INTER S`);
 (SET_TAC[]);
 (ASM_REWRITE_TAC[]);
