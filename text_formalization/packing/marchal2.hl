(* Lemma 1 *)
let AFF_GE_2_2 = prove
 (`!x v w.
        DISJOINT {x,v} {w,z}
        ==> aff_ge {x,v} {w, z} =
             {y | ?t1 t2 t3 t4.
                     &0 <= t3 /\ &0 <= t4 /\
                     t1 + t2 + t3 + t4 = &1 /\
                     y = t1 % x + t2 % v + t3 % w + t4 % z}`,
  AFF_TAC);;
