let UPPER_BOUND_GAMMAX = prove (
 `!V. ?c. (!X.
           saturated V /\
           packing V /\
           mcell_set V X /\
           critical_edgeX V X = {}
           ==> gammaX V X lmfun <= c)`, KY_CHEAT_TAC);;

let NUMBER_OF_MCELLS_AT_BOUNDARY = prove (
 `!V p k1 k2.
         saturated V /\ packing V
         ==> (?C. !r. &1 <= r
                   ==> &(CARD
                         ({X | mcell_set V X /\ X SUBSET ball (p,r + k1)} DIFF
                          {X | mcell_set V X /\ X SUBSET ball (p,r - k2)})) <=
                       C * r pow 2)`, KY_CHEAT_TAC);;

let NUMBER_OF_MCELLS_HAS_SAME_HD = prove (
 `!V u0.
     saturated V /\ packing V
     ==> (?c. !r. &1 <= r
                   ==> &(CARD 
                         {X | mcell_set V X /\                   
                            (?i ul. X = mcell i V ul /\ HD ul = u0)}) <= c)`, KY_CHEAT_TAC);;

