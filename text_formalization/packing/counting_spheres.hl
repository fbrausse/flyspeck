BY(ASM_MESON_TAC []);
HASH_UNDISCH_TAC 9516 ;
ASM_REWRITE_TAC [];
  DISCH_THEN SUBST1_TAC;
HASH_UNDISCH_TAC 6171  ;
  DISCH_THEN (fun _t -> ASSUME_TAC _t THEN ASSUME_TAC _t);
