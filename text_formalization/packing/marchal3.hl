 (MATCH_MP_TAC REAL_LE_MUL);
 (REWRITE_TAC[REAL_LE_POW_2]);
 (REAL_ARITH_TAC);
 (ASM_REAL_ARITH_TAC);

 (NEW_GOAL `&(CARD (int_ball x (r + k1) DIFF int_ball x (r - k2))) <= 
   &(CARD (int_ball x (r + k1)))`);
 (REWRITE_TAC[REAL_OF_NUM_LE]);
 (MATCH_MP_TAC CARD_SUBSET);
