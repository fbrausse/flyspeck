let except_list = new_definition `except_list L (v:A) = 
  LENGTH (FILTER (\f. 5 <= LENGTH f) (facesAt_list L v))`;;

let MEM_EQ_NIL = prove_by_refinement(
  `!s. s = [] <=> (!x. ~MEM x s)`,
