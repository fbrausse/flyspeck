  REWRITE_TAC[INJ;SURJ;IN_ELIM_THM] THEN REPEAT WEAKER_STRIP_TAC THEN ASM_REWRITE_TAC[IN_ELIM_THM];
  REPEAT WEAKER_STRIP_TAC;
