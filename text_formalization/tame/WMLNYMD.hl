      TYPIFY `list_pairs f` EXISTS_TAC;
      ASM_REWRITE_TAC[];
      GMATCH_SIMP_TAC dart_of_find_face;
      REWRITE_TAC[List_hypermap.size_list_pairs];
      ASM_REWRITE_TAC[Seq.size];
      CONJ2_TAC;
