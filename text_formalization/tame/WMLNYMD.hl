let quad_list = new_definition `quad_list L (v:A) = 
  LENGTH (FILTER (\f. LENGTH f = 4) (facesAt_list L v))`;;

let vertextype_list = new_definition `vertextype_list L (v:A) = 
  tri_list L v, quad_list L v, except_list L v`;;

