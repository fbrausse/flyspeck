let nonnil_partition = prove_by_refinement(
  `!n r. all (\c. ~(c = [])) (partition_into_atom n r)`,
  (* {{{ proof *)
  [
  REWRITE_TAC[partition_into_atom];
  REPEAT WEAKER_STRIP_TAC THEN MATCH_MP_TAC nonnil_partition0;
