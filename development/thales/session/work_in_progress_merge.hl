  COMMENT "5a. second half of split";
  EXISTS_TAC `&2`;
  CONJ_TAC;
    BY(COND_CASES_TAC THEN REAL_ARITH_TAC);
  FIRST_X_ASSUM_ST `x / &2 ` MP_TAC;
  REWRITE_TAC[Sphere.ineq;TAUT `(a ==> b ==> c) <=> (a /\ b ==> c)`];
  DISCH_THEN MATCH_MP_TAC;
