    BY(ASM_MESON_TAC[Nonlinear_lemma.sqrt8_sqrt2;Sphere.sqrt2;y_bounds]);
  SUBGOAL_THEN `~critical_edge_y y6` ASSUME_TAC;
    BY(ASM_REWRITE_TAC[Sphere.critical_edge_y;arith `x <= y <=> ~(y < x)`]);
  SUBGOAL_THEN `wtcount3_y y1 y2 y6 =  if critical_edge_y y2 then 2 else 1` ASSUME_TAC;
    ASM_REWRITE_TAC[Sphere.wtcount3_y];
    BY(COND_CASES_TAC THEN TRY (ARITH_TAC));
  COMMENT "4. apply undiv_gamma3";
  MATCH_MP_TAC (arith `a * b < c ==> b * a <= c`);
