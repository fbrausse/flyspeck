  [
  LIST_INDUCT_TAC THEN ASM_REWRITE_TAC[pairwisel;MAP;o_THM];
  REPEAT WEAKER_STRIP_TAC THEN COND_CASES_TAC THEN ASM_REWRITE_TAC[MAP_EQ_NIL];
  BY(ASM_SIMP_TAC[HD_MAP])
  ]);;
  (* }}} *)


