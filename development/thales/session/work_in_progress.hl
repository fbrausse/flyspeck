let taum_attains_inf = prove_by_refinement(
  `!y1 y2 y3 y4 y5 y6. &2 <= y1 /\ y1 <= &2 * h0 /\
	&2 <= y2 /\ y2 <= &2 * h0 /\
	&2 <= y2 /\ y3 <= &2 * h0 /\
	&2 <= y3 /\ y4 <= &2 * h0 /\
	&2 <= y4 /\ y5 <= &2 * h0 /\
	&2 <= y5 /\ y6 <= &2 * h0 
	==>
	(?y4'. &2 <= y4' /\ y4' <= &2 * h0 /\ (!y4. &2 <= y4 /\ y4 <= &2 * h0 ==>
	   taum y1 y2 y3 y4' y5 y6 <= taum y1 y2 y3 y4 y5 y6))`,
  (* {{{ proof *)
  [
  st/r
intro REAL_CONTINUOUS_ATTAINS_INF [`\q. taum y1 y2 y3 q y5 y6`;`real_interval [&2, &2 * h0]`]
  ]);;
  (* }}} *)


