 let I_1305126631 (* Flypaper, Packing, HJKDESR_3cell *) = 
  all_forall `ineq
  [ 
    (#2.0,y1,s8);
    (#2.0,y2,s8);
    (#2.0,y3,s8)
  ]
((eta_y y1 y2 y3 >= s2) \/
  (vol_x (y1*y1) (y2*y2) (&2) (&2) (&2) (y3*y3)
   - (&2 * mm1 / pi) * 
        (sol_y y1 y2 s2 s2 s2 y3 +
	   sol_y y2 y3 s2 s2 s2 y1 +
	   sol_y y3 y1 s2 s2 s2 y2)
    + (&8 * mm2/pi) *
       (marchal_quartic(y1/ &2)* dih_y y1 y2 s2 s2 s2 y3 +
	  marchal_quartic(y2/ &2)* dih_y y2 y3 s2 s2 s2 y1 +
	  marchal_quartic(y3/ &2)* dih_y y3 y1 s2 s2 s2 y2)
         >= &0))`;;


 let I_3148025108 (* Flypaper, Packing, HJKDESR 2cell *) = 
  all_forall `ineq
  [     (#1.0,h,s2) ]
  ((h*(&2- h pow 2)/ &3)  - (&2 * mm1 / pi) *   (&1- h/ (sqrt (&2)))
    + (&8 * mm2/pi) *marchal_quartic h  >= &0)`;;

